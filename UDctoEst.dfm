  object qCor: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      'SELECT dbProdutoCor.CodCor, Dbcor.Nome'
      'FROM dbProdutoCor'
      '   INNER JOIN "Dbcor.db" Dbcor'
      '   ON  (Dbcor.Codigo = dbProdutoCor.CodCor)  '
      'WHERE  (dbProdutoCor.CodProduto = :CodProd)'
      '      AND  (dbProdutoCor.Inativo = False)'
      'ORDER BY Dbcor.Nome')
